var xa,ya,xb,yb, xc, yc: real;
begin
  write('Введите x-координату начальной точки: ');
  read(xa);
  write('Введите y-координату начальной точки: ');
  read(ya);
  write('Введите x-координату конечной точки: ');
  read(xb);
  write('Введите y-координату конечной точки: ');
  read(yb);
  xc := (xa+xb)/2;
  yc := (ya+yb)/2;
  write('Координаты середины между двумя точками: [',xc,',', yc, ']');
end.